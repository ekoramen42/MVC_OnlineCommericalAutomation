@model MVC_OnlineCommericalAutomation.Models.Sınıflar.Kategori
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br/>
<h2>Yeni Kategori Sayfası</h2>
<br />
